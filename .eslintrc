{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "globals": {
    "QB": true,
    "Promise": true
  },
  "rules": {
    "strict": [2, "global"],
    "no-unused-vars": [2, { "args": "none" }],
    "no-labels": 0,
    "semi": 2,
    "no-empty-label": 0,
    "no-alert": 2,
    "no-console": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-extra-boolean-cast": 1
    }
}
